<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minboard.mapper.MemberMapper">

    <resultMap id="memberVoResultMap" type="MemberVo">
        <result property="createTime" column="create_time"/>
    </resultMap>


    <!--- 회원가입  -->
    <insert id="createMember"
            parameterType="MemberVo"
            useGeneratedKeys ="true"
            keyProperty="id">

        insert into member(email, password, member_name, birthday, gender, create_time, update_time)
        values (#{email}, #{password}, #{name}, #{birthday}, #{gender}, #{createTime}, #{update_time})
    </insert>


</mapper>